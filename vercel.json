{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "regions": ["bom1"],
  "github": {
    "enabled": true,
    "autoJobCancelation": true,
    "autoAlias": true
  }
}
